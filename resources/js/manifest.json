{
  "name": "ammo-inteceptor",
  "version": "0.0.1",
  "browser_action": {
      "default_icon": {
        "19": "icon19.png",
        "38": "icon38.png"
      }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["ammo_inteceptor.js", "background.js"],
    "persistent": true 
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}

